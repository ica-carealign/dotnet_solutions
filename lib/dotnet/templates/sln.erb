
Microsoft Visual Studio Solution File, Format Version 12.00
# Visual Studio 2013
VisualStudioVersion = 12.0.30723.0
MinimumVisualStudioVersion = 10.0.40219.1
<% projects.map { |project| project.model }.each do |project| %>
Project("{<%= project.language %>}") = "<%= project.name %>", "<%= project.name %>\<%= project.file_name %>", "{<%= project.id %>}"
EndProject
<% end %>
Global
  GlobalSection(SolutionConfigurationPlatforms) = preSolution
    Debug|Any CPU = Debug|Any CPU
    Debug|Mixed Platforms = Debug|Mixed Platforms
    Debug|x86 = Debug|x86
    Release|Any CPU = Release|Any CPU
    Release|Mixed Platforms = Release|Mixed Platforms
    Release|x86 = Release|x86
  EndGlobalSection
  GlobalSection(ProjectConfigurationPlatforms) = postSolution
<% projects.map { |project| project.model }.each do |project| %>
    {<%= project.id %>}.Debug|Any CPU.ActiveCfg = Debug|Any CPU
    {<%= project.id %>}.Debug|Any CPU.Build.0 = Debug|Any CPU
    {<%= project.id %>}.Debug|Mixed Platforms.ActiveCfg = Debug|Any CPU
    {<%= project.id %>}.Debug|Mixed Platforms.Build.0 = Debug|Any CPU
    {<%= project.id %>}.Debug|x86.ActiveCfg = Debug|Any CPU
    {<%= project.id %>}.Release|Any CPU.ActiveCfg = Release|Any CPU
    {<%= project.id %>}.Release|Any CPU.Build.0 = Release|Any CPU
    {<%= project.id %>}.Release|Mixed Platforms.ActiveCfg = Release|Any CPU
    {<%= project.id %>}.Release|Mixed Platforms.Build.0 = Release|Any CPU
    {<%= project.id %>}.Release|x86.ActiveCfg = Release|Any CPU
<% end %>
  EndGlobalSection
  GlobalSection(SolutionProperties) = preSolution
    HideSolutionNode = FALSE
  EndGlobalSection
EndGlobal
